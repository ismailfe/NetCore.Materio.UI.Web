<environment include="Development">
    <script src="~/lib/jquery/jquery.js"></script>
    <script src="~/vendor/popper/popper.js"></script>
    <script src="~/lib/bootstrap/js/bootstrap.js"></script>
  <!-- `perfect-scrollbar` library required by SideNav plugin -->
    <script src="~/vendor/perfect-scrollbar/perfect-scrollbar.js"></script>
    <script src="~/vendor/node-waves/node-waves.js"></script>
    <script src="~/vendor/menu/menu.js"></script>
  <script src="~/js/site.js"></script>
</environment>
<environment exclude="Development">
    <script src="~/lib/jquery/jquery.js" asp-append-version="true"></script>
    <script src="~/vendor/popper/popper.js" asp-append-version="true"></script>
    <script src="~/lib/bootstrap/js/bootstrap.js" asp-append-version="true"></script>
    <script src="~/vendor/perfect-scrollbar/perfect-scrollbar.js" asp-append-version="true"></script>
    <script src="~/~/vendor/node-waves/node-waves.js" asp-append-version="true"></script>
    <script src="~/vendor/menu/menu.js" asp-append-version="true"></script>
  <script src="~/js/site.js" asp-append-version="true"></script>
</environment>
